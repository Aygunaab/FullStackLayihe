@model TeamMember
@{
    ViewData["Title"] = "DetailTeamMember";
}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">@Model.FullName</h4>
                        <h4 class="card-title">@Model.Profession</h4>

                        <p>
                            Image:
                            <img style="width: 300px;" src="~/Uploads/@Model.Image" />
                        </p>


                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

